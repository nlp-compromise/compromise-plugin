{
  "name": "compromise-plugin",
  "description": "author and compress nlp-compromise plugins",
  "version": "0.0.8",
  "author": "Spencer Kelly <spencermountain@gmail.com> (http://spencermounta.in)",
  "repository": {
    "type": "git",
    "url": "git://github.com/nlp-compromise/compromise-plugin.git"
  },
  "main": "./builds/compromise-plugin.js",
  "bin": {
    "compromise-pack": "./bin/pack.js",
    "compromise-unpack": "./bin/unpack.js"
  },
  "scripts": {
    "test": "\"node_modules/.bin/tape\" \"./tests/*.test.js\" | \"node_modules/.bin/tap-spec\" --color",
    "build": "node ./scripts/build.js",
    "watch": "./node_modules/.bin/amble ./scratch.js "
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "peerDependencies": {
    "compromise": "^11.0.0"
  },
  "dependencies": {
    "efrt": "^2.0.3"
  },
  "devDependencies": {
    "amble": "0.0.7",
    "babel-preset-es2015": "^6.24.1",
    "browserify": "16.2.3",
    "chalk": "^2.1.0",
    "derequire": "^2.0.6",
    "shelljs": "0.8.3",
    "tap-spec": "5.0.0",
    "tape": "^4.8.0",
    "uglify-js": "^3.1.4",
    "uglifyify": "5.0.1"
  },
  "license": "MIT"
}
